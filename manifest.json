{
  "name": "Feature Policy Tester DevTools Extension",
  "manifest_version": 2,
  "description": "DevTools extension to test Feature Policy settings",
  "version": "0.0.1",
  "devtools_page": "page.html",
  "background": {
    "persistent": true,
    "scripts": ["background.js"]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ]
}
